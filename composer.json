{
    "name": "ronu/laravel-openapi-generator",
    "description": "Automatic OpenAPI 3.0.3 documentation generator for Laravel with Postman and Insomnia export support",
    "version": "1.2.3",
    "type": "library",
    "keywords": [
        "laravel",
        "openapi",
        "swagger",
        "api-documentation",
        "postman",
        "insomnia",
        "rest-api",
        "documentation-generator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "charlietyn",
            "email": "charlietyn@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "cebe/php-openapi": "^1.7",
        "symfony/yaml": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Ronu\\OpenApiGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ronu\\OpenApiGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ronu\\OpenApiGenerator\\Providers\\OpenApiGeneratorServiceProvider"
            ],
            "aliases": {
                "OpenApiGenerator": "Ronu\\OpenApiGenerator\\Facades\\OpenApiGenerator"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "analyse": "vendor/bin/phpstan analyse src --level=8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
